  "devDependencies": {
    "chai": "4.1.x",
    "chai-spies": "1.0.x",
    "chalk": "2.4.x",
    "cross-var": "1.1.x",
    "jsdom": "12.0.0",
    "jsdom-global": "3.0.x",
    "mocha": "5.2.x",
    "nyc": "13.0.x",
    "rfr": "1.2.x",
